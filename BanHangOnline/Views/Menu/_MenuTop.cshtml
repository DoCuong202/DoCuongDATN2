@model IEnumerable<BanHangOnline.Models.EF.Category>

<style>
    .navbar_menu li a {
        font-family: Texturina !important;
        font-size: 13px !important;
    }
</style>
@if (Model != null && Model.Any())
{
    <ul class="navbar_menu">
        @foreach (var item in Model)
        {
            if (item.Alias.Equals("trang-chu"))
            {
                item.Alias = "";
            }
            <li><a href="/@item.Alias">@item.Title</a></li>
        }
    </ul>
}
